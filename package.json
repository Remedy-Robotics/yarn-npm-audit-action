{
  "name": "npm-audit-action",
  "version": "2.4.4",
  "private": true,
  "description": "GitHub Action to run `npm audit`",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test -- --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oke-py/npm-audit-action"
  },
  "keywords": [
    "actions",
    "node",
    "security",
    "vulnerability"
  ],
  "author": "Naoki Oketani <okepy.naoki@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "@octokit/rest": "^19.0.11",
    "strip-ansi": "^6.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.2.4",
    "@types/node": "^18.0.0",
    "@typescript-eslint/parser": "^5.47.1",
    "@vercel/ncc": "^0.36.0",
    "eslint": "^8.30.0",
    "eslint-plugin-github": "^4.6.0",
    "eslint-plugin-jest": "^27.1.7",
    "graphql": "^16.8.1",
    "jest": "^29.3.1",
    "jest-circus": "^29.3.1",
    "js-yaml": "^4.0.0",
    "prettier": "^2.8.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.0"
  }
}
